{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Launch and debug gem5 in se mode",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/X86/gem5.debug",
            "args": [
                
                //"--debug-flag=SMS",
                "configs/learning_gem5/newBench.py" ,
                //"--fast-forward=100000000000000000",
                "--cmd='tests/test-progs/threadHello/LU;tests/test-progs/threadHello/LU;tests/test-progs/threadHello/LU;tests/test-progs/threadHello/LU;'",
                //"--cmd='/home/tanawin/Work/Hwsim/netBench/cmake-build-debug/netBench1;/home/tanawin/Work/Hwsim/netBench/cmake-build-debug/netBench1;/home/tanawin/Work/Hwsim/netBench/cmake-build-debug/netBench1;/home/tanawin/Work/Hwsim/netBench/cmake-build-debug/netBench1'",
                "--cpu-type=DerivO3CPU" ,
                "--l1d_size=1kB",
                "--l1i_size=16kB",
                "--caches",
                "-n 4 ",
                "--interQmemSched=STAGE_SCHED_Queue"
                //"--memSched=frfcfs",                // "--interQmemSizeHelp=STAGE"

            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [{
                "name": "M5_PATH",
                "value": "${workspaceFolder}"
            }],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
    ]
}