{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Launch and debug gem5 in se mode",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/X86/gem5.debug",
            "args": [
                
                "--debug-flag=PseudoInst,MY_SYS,mydelay",
                "configs/learning_gem5/newBench.py" ,
                "--cmd=/home/tanawin/Work/Hwsim/netBench/cmake-build-debug/waitBench1" ,
                "--cpu-type=DerivO3CPU" ,
                "--l1d_size=64kB",
                "--l1i_size=16kB",
                "--caches",
                "-n 1 ",
                "--interQmemSched=SimpleQueue"

            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [{
                "name": "M5_PATH",
                "value": "${workspaceFolder}"
            }],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
    ]
}